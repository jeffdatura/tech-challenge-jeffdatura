{% extends 'base.html.twig' %}

{% block title %}Proprio index{% endblock %}

{% block body %}
    <h1>Proprio index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for proprio in proprios %}
            <tr>
                <td>{{ proprio.id }}</td>
                <td>{{ proprio.nom }}</td>
                <td>
                    <a href="{{ path('app_proprio_show', {'id': proprio.id}) }}">show</a>
                    <a href="{{ path('app_proprio_edit', {'id': proprio.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_proprio_new') }}">Create new</a>
{% endblock %}
